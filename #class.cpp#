#include<iostream>
#include<cstdlib>
#include<algorithm>
using namespace std;
int main(void){
  int N,Q;
  cin >> N;
  int A[N];
  for(int i = 0 ; i < N ; i++ ){
    cin >> A[i];
  }
  cin>>Q;
  int B[Q];
  for(int i = 0 ; i < Q ; i++ ){
    cin >> B[i];
  }
  int func[N];
  for(int i = 0 ; i < Q ; i++ ){
    for(int j = 0 ; j < N ; j++ ){
      func[j] = abs( B[i] - A[j] );
    }
    cout<< *min_element(func,func+N) <<endl;
  }   
  return 0;
}
      
      
