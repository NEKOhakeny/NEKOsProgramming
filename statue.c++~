#include<iostream>
#include<cmath>
#define pi 3.14159

using  namespace std;

int main(void){
  int t;
  double l,x,y;
  int q;
  cin>>t>>l>>x>>y;
  cin>>q;
  for(int i = 0; i < q ; i++){
    int e;
    cin>>e;
    double T = e % t;
    double wt = T / t * pi;
    double sinwt = sin( wt );
    double coswt = cos( wt );
    double bottom = hypot(x,y - l * coswt);
    double height = l * sinwt;
    double tanjent = height / bottom;
    double atanjent = atan(tanjent);
    double ans = atanjent * 180 / pi;
    cout<<ans<<endl;
  }
  return 0;
}
