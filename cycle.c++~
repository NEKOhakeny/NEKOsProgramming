#include<iostream>
#define rep(a,b,c) for(int a = b; a < c ; a++)
using namespace std;
typedef strct{
    int num[2];
    char symbol;
    int ans;
    if(symbol == '*')ans = num[0] * num[1];
    if(symbol == '+')ans = num[0] + num[1];
    if(symbol == '-')ans = num[0] - num[1];
    if(symbol == '/')ans = num[0] / num[1];
    int structor(int ans,int add){
        cin>>symbol;
         if(symbol == '*') ans *= add;

    }
    
    
}poland;
int main(void){
    poland opeland[50];
    rep(i,0,50){
        cin>>opeland[i].num[0];
        cin>>opeland[i].num[1];
        cin>>opeland[i].symbol;
    }


    return 0;
}
