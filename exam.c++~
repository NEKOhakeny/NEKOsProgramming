#include<iostream>

using namespace std;

int main(void){
  rep(i,0,n - 1){
         rep(j,0,n - i){
             if(number[n - 2 - j] > number[n-1-j]){
                 
                 int a = number[n-2-j];
                 number[n-2-j] = number[n-1-j];
                 number[n-1-j] = a;
                 char c = bubble[n-2-j];
                 bubble[n-2-j] = bubble[n-1-j];
                 bubble[n-1-j] = c;
    
             }
         }
     }
  return 0;
}
